<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <title>Document</title>
</head>
<body>
    <div  class="w3-container">
        <h2>Details User</h2>
        <table  class="w3-table-all w3-hoverable">
            <thead>
                <tr class="w3-light-grey">
                    <th>id</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>email</th>
                    <th>address</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td><%= dataDetailUser[0][0].id %></td>
                    <td><%= dataDetailUser[0][0].firstName %></td>
                    <td><%= dataDetailUser[0][0].lastName %></td>
                    <td><%= dataDetailUser[0][0].email %></td>
                    <td><%= dataDetailUser[0][0].address %></td>
                </tr>
            </tbody>
        </table>
        <a href="/">Back</a>
    </div>
</body>
</html>